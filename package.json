{
  "name": "@pureskillgg/ace",
  "version": "0.2.0",
  "description": "Loads and caches configuration and secrets from AWS services.",
  "type": "module",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./index.js",
    "require": "./dist/index.cjs"
  },
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/pureskillgg/ace",
  "bugs": "https://github.com/pureskillgg/ace/issues",
  "repository": "pureskillgg/ace",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": {
    "name": "Evan Sosenko",
    "email": "evan@pureskill.gg"
  },
  "files": [
    "yarn.lock",
    "index.js",
    "lib",
    "data",
    "dist",
    "docs"
  ],
  "scripts": {
    "test": "nyc ava",
    "test:update": "ava --update-snapshots",
    "test:watch": "ava --watch",
    "test:debug": "ava debug --break",
    "lint": "npm-run-all --parallel lint:*",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.(js|json|yml|yaml|graphql)'",
    "lint:js": "eslint --ignore-path .gitignore --ext .js .",
    "clean": "del dist .dist",
    "build": "npm-run-all --parallel build:*",
    "prebuild": "npm-run-all clean",
    "build:index": "babel --source-maps --out-file-extension .cjs --out-dir dist index.js",
    "build:lib": "babel --source-maps --out-file-extension .cjs --ignore='**/*.spec.js' --out-dir dist/lib lib",
    "build:spec": "npm-run-all --parallel build:spec:*",
    "build:spec:index": "babel --source-maps --out-file-extension .cjs --out-dir .dist index.js",
    "build:spec:lib": "babel --source-maps --out-file-extension .cjs --out-dir .dist/lib lib",
    "build:spec:test": "babel --source-maps --out-file-extension .cjs --out-dir .dist/test test",
    "preversion": "npm-run-all --parallel lint test build",
    "postversion": "git push --follow-tags",
    "example": "node examples",
    "example:inspect": "node --inspect examples",
    "format": "npm-run-all --parallel format:*",
    "preformat": "prettier --write --ignore-path .gitignore '**/*.(js|json|yml|yaml|graphql)'",
    "format:js": "eslint --ignore-path .gitignore --fix --ext .js .",
    "report": "nyc report"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "dependencies": {
    "@meltwater/mlabs-logger": "^7.0.1",
    "@meltwater/phi": "^3.10.0",
    "aws-sdk": "^2.814.0",
    "cache-manager": "^3.4.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.0.0",
    "@meltwater/examplr": "^5.0.1",
    "ava": "^3.3.0",
    "babel-plugin-add-import-extension": "^1.4.3",
    "del-cli": "^3.0.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "testdouble": "^3.16.1"
  }
}
