{
  "name": "@pureskillgg/makenew-jsmodule",
  "version": "2.3.7",
  "description": "Package skeleton for a JavaScript module.",
  "type": "module",
  "main": "index.js",
  "exports": {
    "import": "./index.js"
  },
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/pureskillgg/makenew-jsmodule",
  "bugs": "https://github.com/pureskillgg/makenew-jsmodule/issues",
  "repository": "pureskillgg/makenew-jsmodule",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": {
    "name": "PureSkill.gg",
    "email": "secure@pureskill.gg"
  },
  "files": [
    "yarn.lock",
    "index.js",
    "lib",
    "data",
    "docs"
  ],
  "scripts": {
    "test": "nyc ava",
    "test:update": "ava --update-snapshots",
    "test:watch": "ava --watch",
    "test:debug": "ava debug --break",
    "lint": "npm-run-all --parallel lint:*",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.(js|json|yml|yaml|graphql)'",
    "lint:js": "eslint --ignore-path .gitignore --ext .js .",
    "preversion": "npm-run-all --parallel lint test",
    "postversion": "git push --follow-tags",
    "example": "node examples",
    "example:inspect": "node --inspect examples",
    "format": "npm-run-all --parallel format:*",
    "preformat": "prettier --write --ignore-path .gitignore '**/*.(js|json|yml|yaml|graphql)'",
    "format:js": "eslint --ignore-path .gitignore --fix --ext .js .",
    "report": "nyc report"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@meltwater/examplr": "^6.0.0",
    "ava": "^3.3.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2"
  }
}
