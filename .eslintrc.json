{
  "root": true,
  "extends": ["standard"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": ["@babel/plugin-proposal-class-properties"]
    }
  },
  "rules": {
    "valid-jsdoc": "error",
    "space-before-function-paren": "off"
  }
}
